[alias]
	sh = !/usr/local/bin/git-sh
	edit = !/usr/lib/git-core//git-edit
	forest = !/usr/lib/git-core//git-forest
    st = status
    pl = pull --rebase origin
    fo = fetch origin
    ph = push origin
    co = checkout
    ro = rebase origin/master

[color]
    ui = auto

[user]
    name = Alexey Terentyev
    email = alexey@terentyev.pro

[core]
    excludesfile = ~/.config/git/gitignore
